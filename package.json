{
  "name": "buscacode-service-nest",
  "version": "0.0.1",
  "description": "",
  "author": "Wilder Trujillo",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs -d src/database/typeorm.config.ts migration:create",
    "migration:generate": "typeorm-ts-node-commonjs -d src/database/typeorm.config.ts migration:generate",
    "migration:run": "typeorm-ts-node-commonjs -d src/database/typeorm.config.ts migration:run",
    "migration:show": "typeorm-ts-node-commonjs -d src/database/typeorm.config.ts migration:show"
  },
  "dependencies": {
    "@apollo/server": "4.12.2",
    "@nestjs/apollo": "13.1.0",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/graphql": "13.1.0",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/typeorm": "11.0.0",
    "dotenv": "17.2.1",
    "graphql": "16.11.0",
    "mysql2": "3.14.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typeorm": "0.3.26",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.34.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.6",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.0",
    "@types/supertest": "6.0.3",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.3.0",
    "jest": "30.1.1",
    "prettier": "3.6.2",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.12.3+sha512.467df2c586056165580ad6dfb54ceaad94c5a30f80893ebdec5a44c5aa73c205ae4a5bb9d5ed6bb84ea7c249ece786642bbb49d06a307df218d03da41c317417"
}
